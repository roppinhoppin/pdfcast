#!/usr/bin/env python3
import os
import subprocess

# Execute retrieve.py
subprocess.run(["python", "retrieve.py"])

# Add changes to git
subprocess.run(["git", "add", "."])

# Commit changes
commit_message = "Update after running retrieve.py"
subprocess.run(["git", "commit", "-m", commit_message])

# Push changes
subprocess.run(["git", "push"])